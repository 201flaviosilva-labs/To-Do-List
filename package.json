  "scripts": {
    "clear": "rm -rf build && rm -rf out && rm -rf dist",
    "start": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "react-scripts test --coverage",
    "test:nowatch": "npm test -- --watchAll=false",
    "lint": "eslint ./src/ --fix",
    "analyze": "npm run build && source-map-explorer 'dist/js/*.js' --html ./source-map-explorer/result.html && source-map-explorer 'dist/js/*.js' --json ./source-map-explorer/result.json",
    "depcheck": "depcheck",
    "bump": "npm run build && np --no-tests --any-branch --no-publish"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "css-free-style": "^1.3.0",
