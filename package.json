    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "redux": "^4.2.1",
    "styled-components": "^5.3.10",
    "sweetalert2": "^11.7.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-redux": "^7.1.25",
