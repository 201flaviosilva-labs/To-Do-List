    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-styled-components-a11y": "^2.0.0",
    "jest": "^29.5.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-plugin-eslint": "^1.8.1"
